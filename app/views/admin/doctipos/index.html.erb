<p id="notice"><%= notice %></p>

<h1>Listing Admin Doctipos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_doctipos.each do |admin_doctipo| %>
      <tr>
        <td><%= admin_doctipo.nombre %></td>
        <td><%= link_to 'Show', admin_doctipo_path(admin_doctipo) %></td>
        <td><%= link_to 'Edit', edit_admin_doctipo_path(admin_doctipo) %></td>
        <td><%= link_to 'Destroy', [:admin, admin_doctipo] , method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Doctipo', new_admin_doctipo_path %>
