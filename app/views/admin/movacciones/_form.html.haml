= form_for [:admin,@admin_movaccion] do |f|
  - if @admin_movaccion.errors.any?
    #error_explanation
      %h2= "#{pluralize(@admin_movaccion.errors.count, "error")} prohibited this admin_movaccion from being saved:"
      %ul
        - @admin_movaccion.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :nombre
    = f.text_field :nombre
  .actions
    = f.submit 'Guardar',{class: 'button'}
