<p id="notice"><%= notice %></p>

<h1>Listing Admin Docestados</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_docestados.each do |admin_docestado| %>
      <tr>
        <td><%= admin_docestado.nombre %></td>
        <td><%= link_to 'Show', [:admin, admin_docestado] %></td>
        <td><%= link_to 'Edit', edit_admin_docestado_path(admin_docestado) %></td>
        <td><%= link_to 'Destroy', [:admin, admin_docestado], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Docestado', new_admin_docestado_path %>
